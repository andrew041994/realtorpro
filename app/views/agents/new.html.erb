<center><h1>Sign Up</h1>

<%= form_for @agent do |f|%>

<%= f.label :first_name%>
<%= f.text_field :first_name%>
<br><br>
<%= f.label :last_name%>
<%= f.text_field :last_name%>
<br><br>
<%= f.label :username%>
<%= f.text_field :username%>
<br><br>
<%= f.label :email%>
<%= f.email_field :email%>
<br><br>
<%= f.label :company%>
<%= f.text_field :company%>
<br><br>
<%= f.label :password%>
<%= f.password_field :password%>
<br><br>

<%=f.submit%>
<% end %>
</center>